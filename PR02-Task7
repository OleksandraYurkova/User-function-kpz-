#Task7
def countNumbers():
    print("Task7")
    a = int(input("Введіть число a: "))
    b = int(input("Введіть число b: "))
    c = int(input("Введіть число c: "))

    count = 0
    if a < 0:
        count += 1
    if b < 0:
        count += 1
    if c < 0:
        count += 1

    return count
result = countNumbers()
print(f"Кількість від'ємних чисел: {result}")
